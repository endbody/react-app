@import("../stylus/index.styl")

.header
    position: fixed
    left: 0
    top: 0
    z-index: 1
    
    width: 100vw;
    height $headerHeight
    background-color $primary
    line-height: $headerHeight
    color: $font-colorFFF
    .header-title
        position: absolute
        text-align: center
        width:100%
        height 100%
        z-index: 2
    .header-register
        position: absolute
        right: .14rem
        z-index: 3
    .header-back
        position: absolute
        left :0.14rem
        z-index: 3